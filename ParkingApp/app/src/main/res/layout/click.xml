<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/click_ParkingNaam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="34dp"
            android:textSize="32dp"
            tools:layout_editor_absoluteY="26dp" />

        <TextView
            android:id="@+id/click_ParkingHuidige"
            android:layout_width="wrap_content"
            android:layout_weight="0.2"
            android:textSize="28dp"
            android:textColor="#000000"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="int"
/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        >


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Naam"
        android:textColor="#7158e2"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="60dp"
        tools:text="Naam" />

    <TextView
        android:id="@+id/click_ParkingDesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="97dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adres"
        android:textColor="#7158e2"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="138dp" />

        <TextView
            android:id="@+id/click_ParkingAdress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:onClick="maps"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="178dp" />

        <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact Informatie"
            android:textColor="#7158e2"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/click_ParkingContactInformatie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="256dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Totale Capaciteit"
        android:textColor="#7158e2"

        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="299dp" />


        <TextView
            android:id="@+id/click_ParkingTotaalCap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="333dp" />
    </LinearLayout>
</LinearLayout>

